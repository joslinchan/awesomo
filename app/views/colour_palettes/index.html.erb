<h1>Palette Collection</h1>
<ul>
  <% @paletteCollection.each do |palette| %>
    <li>
      <%= link_to(
        palette.title,
        palette.url
      ) %>
      <br />
       <%= link_to(
         image_tag(palette.image_url),
         palette.url
       ) %>
      <br />
       <%= link_to(
         "Delete",
         colour_palette_path(palette.id),
         method: :delete,
         data: {
           confirm:"Are you sure?"
         }
        ) %>
    </li>
  <% end %>
</ul>
